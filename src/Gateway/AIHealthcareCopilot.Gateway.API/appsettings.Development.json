{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Information"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "auth-route": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/auth/{**catch-all}"
        }
      },
      "patients-route": {
        "ClusterId": "patients-cluster",
        "Match": {
          "Path": "/api/patients/{**catch-all}"
        }
      },
      "doctors-route": {
        "ClusterId": "patients-cluster",
        "Match": {
          "Path": "/api/doctors/{**catch-all}"
        }
        },
        "analysisresults-route": {
          "ClusterId": "patients-cluster",
          "Match": {
            "Path": "/api/analysisresults/{**catch-all}"
          }
      },
      "analysis-route": {
        "ClusterId": "analysis-cluster",
        "Match": {
          "Path": "/api/analysis/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "auth-destination": {
            "Address": "https://localhost:7262/"
          }
        }
      },
      "patients-cluster": {
        "Destinations": {
          "patients-destination": {
            "Address": "https://localhost:7174/"
          }
        }
      },
      "analysis-cluster": {
        "Destinations": {
          "analysis-destination": {
            "Address": "https://localhost:7001/"
          }
        }
      }
    }
  }
}